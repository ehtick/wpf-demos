            <local:CellTemplateViewModel x:Key="olapGridDataContext" />

            <Style x:Key="colStyle" TargetType="{x:Type olapgrid:OlapGridTemplateCell}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type olapgrid:OlapGridTemplateCell}">
                            <StackPanel Orientation="Horizontal" Background="White">
                                <!--Image block-->
                                <Image Margin="2" VerticalAlignment="Center" HorizontalAlignment="Center">
                                    <Image.Style>
                                        <Style TargetType="{x:Type Image}">
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding Path=CellValue}" Value="United States">
                                                    <Setter Property="Source" Value="{StaticResource UnitedStates}"/>
                                                    <Setter Property="Width" Value="32"/>
                                                    <Setter Property="Height" Value="28"/>
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding Path=CellValue}" Value="United Kingdom">
                                                    <Setter Property="Source" Value="{StaticResource UnitedKingdom}"/>
                                                    <Setter Property="Width" Value="32"/>
                                                    <Setter Property="Height" Value="32"/>
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding Path=CellValue}" Value="Canada">
                                                    <Setter Property="Source" Value="{StaticResource Canada}"/>
                                                    <Setter Property="Width" Value="32"/>
                                                    <Setter Property="Height" Value="32"/>
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding Path=CellValue}" Value="France">
                                                    <Setter Property="Source" Value="{StaticResource France}"/>
                                                    <Setter Property="Width" Value="32"/>
                                                    <Setter Property="Height" Value="32"/>
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding Path=CellValue}" Value="Germany">
                                                    <Setter Property="Source" Value="{StaticResource Germany}"/>
                                                    <Setter Property="Width" Value="32"/>
                                                    <Setter Property="Height" Value="32"/>
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Image.Style>
                                </Image>

                                <TextBlock Margin="3,4,2,0"
                                           Text="{Binding Path=CellValue}" Foreground="Black"
                                           TextWrapping="Wrap" HorizontalAlignment="Center"
                                           VerticalAlignment="Center" FontFamily="Calibri" FontSize="12"/>
                            </StackPanel>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </Grid.Resources>

        <GroupBox Margin="5" Header="OlapGrid" BorderThickness="1" BorderBrush="DarkGray">
            <GroupBox.HeaderTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding}" FontWeight="Bold"/>
                </DataTemplate>
            </GroupBox.HeaderTemplate>
            <olapgrid:OlapGrid x:Name="olapGrid1" Margin="5" DataContext="{StaticResource olapGridDataContext}"
                               HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ShowHeaderCellsToolTip="True" OlapDataManager="{Binding GridDataManager}">
                <olapgrid:OlapGrid.ColumnHeaderStyle>
                    <olapgrid:OlapGridCellStyle Style="{StaticResource colStyle}"/>
                </olapgrid:OlapGrid.ColumnHeaderStyle>
            </olapgrid:OlapGrid>
        </GroupBox>
    </Grid>
