<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.propertygriddemos.wpfapplication</AssemblyName>
    <RootNamespace>syncfusion.propertygriddemos.wpf</RootNamespace>
  </PropertyGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml">
      <RequiredTargetFramework>4.0</RequiredTargetFramework>
    </Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
    <Reference Include="System.ComponentModel.DataAnnotations" />
    <Reference Include="Syncfusion.PropertyGrid.WPF" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\common\syncfusion.demoscommon.wpf_lib.csproj">
	  <Name>syncfusion.demoscommon.wpf_lib</Name>
	</ProjectReference>
	<ProjectReference Include="syncfusion.propertygriddemos.wpf_lib.csproj">
	  <Name>syncfusion.propertygriddemos.wpf_lib</Name>
	</ProjectReference>
  </ItemGroup>

  <ItemGroup>
    <None Remove="Properties\Settings.settings" />
	<None Remove="Assets\Images\WPF_images.png" />
	<Compile Remove="Assets\**" />
	<Page Remove="Assets\**" />
	<Compile Remove="Build-In Mask Editor\**" />
	<Page Remove="Build-In Mask Editor\**" />
	<Compile Remove="Built-in Editor\**" />
	<Page Remove="Built-in Editor\**" />
	<Compile Remove="Category Editor\**" />
	<Page Remove="Category Editor\**" />
	<Compile Remove="Collection Editor\**" />
	<Page Remove="Collection Editor\**" />
	<Compile Remove="Configure at Compiletime\**" />
	<Page Remove="Configure at Compiletime\**" />
	<Compile Remove="Configure at Runtime\**" />
	<Page Remove="Configure at Runtime\**" />
	<Compile Remove="Converter\**" />
	<Compile Remove="Custom Editor\**" />
	<Page Remove="Custom Editor\**" />
	<Compile Remove="Editors\**" />
	<Compile Remove="Getting Started\**" />
	<Page Remove="Getting Started\**" />
	<Compile Remove="Nested and ReadOnly Support\**" />
	<Page Remove="Nested and ReadOnly Support\**" />
	<Compile Remove="PropertyDefinition\**" />
	<Page Remove="PropertyDefinition\**" />
    <Compile Remove="PropertyGridDemosViewModel.cs" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Syncfusion.PropertyGrid.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.PropertyGrid.WPF.dll</HintPath>
    </Reference>
  </ItemGroup>
  <PropertyGroup>
	<ApplicationIcon>App.ico</ApplicationIcon>
  </PropertyGroup>
  <Import Project="..\common\demos.common.props" />
  <Import Project="..\targets\MultiTargeting.targets" />

</Project>
