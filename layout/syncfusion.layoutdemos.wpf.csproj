    <Compile Remove="Converter\**" />
    <Compile Remove="Document Container\**" />
    <Compile Remove="Grid Splitter\**" />
    <Compile Remove="TextInputLayout\**" />
    <Compile Remove="Tile View\**" />
    <EmbeddedResource Remove="Assets\**" />
