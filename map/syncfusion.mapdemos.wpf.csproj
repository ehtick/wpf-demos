<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.mapdemos.wpfapplication</AssemblyName>
    <RootNamespace>syncfusion.mapdemos.wpf</RootNamespace>
  </PropertyGroup>
  
   <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\common\syncfusion.demoscommon.wpf_lib.csproj" />
    <ProjectReference Include="syncfusion.mapdemos.wpf_lib.csproj" />
    <Reference Include="Syncfusion.SfMaps.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfMaps.WPF.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Views\*" />
    <Page Remove="Views\*" />
    <Compile Remove="ViewModel\*" />
    <Compile Remove="Behaviors\*" />
    <Compile Remove="Model\*" />
    <Compile Remove="Helper\*" />
     <Compile Remove="Service\*" />
    <Compile Remove="Converter\*" />
    <Compile Remove="MapDemosViewModel.cs"/>
    <None Remove="Assets\**"/>
  </ItemGroup>

  <Import Project="..\common\demos.common.props" />
  <Import Project="..\targets\MultiTargeting.targets" />
</Project>