<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<AssemblyName>syncfusion.datagriddemos.wpf.app</AssemblyName>
		<RootNamespace>syncfusion.datagriddemos.wpf</RootNamespace>
	</PropertyGroup>
	<ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
		<RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
	</ItemGroup>
	<ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
		<Reference Include="System" />
		<Reference Include="System.Data" />
		<Reference Include="System.ComponentModel.DataAnnotations" />
		<Reference Include="System.Xml" />
		<Reference Include="System.Runtime.Serialization" />
		<Reference Include="System.ServiceModel" />
		<Reference Include="Microsoft.CSharp" />
		<Reference Include="System.Core" />
		<Reference Include="System.Xml.Linq" />
		<Reference Include="System.Data.DataSetExtensions" />
		<Reference Include="System.Net.Http" />
		<Reference Include="System.Xaml">
		</Reference>
		<Reference Include="WindowsBase" />
		<Reference Include="PresentationCore" />
		<Reference Include="PresentationFramework" />
	</ItemGroup>
	<ItemGroup>
		<Reference Include="Syncfusion.Data.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Data.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.SfChart.Wpf">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfChart.Wpf.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.SfGrid.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfGrid.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.Compression.base">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Compression.base.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.XlsIO.base">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.XlsIO.base.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.Pdf.Base">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Pdf.Base.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.SfGridConverter.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfGridConverter.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.SfProgressBar.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfProgressBar.WPF.dll</HintPath>
		</Reference>
		<PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\common\syncfusion.demoscommon.wpf_lib.csproj">
			<Name>syncfusion.demoscommon.wpf_lib</Name>
		</ProjectReference>
		<ProjectReference Include="syncfusion.datagriddemos.wpf_lib.csproj">
			<Name>syncfusion.datagriddemos.wpf_lib</Name>
		</ProjectReference>
	</ItemGroup>
	<ItemGroup>
		<None Remove="packages.config"/>
		<None Remove="App.config" />
		<None Remove="Properties\Settings.settings" />
		<None Remove="Assets\datagrid\*" />
		<Compile Remove="Views\**\*"/>
		<Page Remove="Views\**\*"/>
		<Compile Remove="Behaviors\*" />
		<Compile Remove="Command\*" />
		<Compile Remove="Converter\*" />
		<Compile Remove="Helpers\*" />
		<Compile Remove="Model\*" />
		<Page Remove="Resources\*" />
		<EmbeddedResource Remove="Resources\Syncfusion.SfGrid.WPF.de-DE.resx" />
		<Compile Remove="Service\SemanticKernelAI.cs" />
		<Compile Remove="ViewModel\*" />
		<Compile Remove="DataGridDemosViewModel.cs" />
	</ItemGroup>
	<Import Project="..\common\demos.common.props">
	</Import>
	<Import Project="..\targets\MultiTargeting.targets" />
</Project>