<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>syncfusion.ganttdemos.wpf</AssemblyName>
    <RootNamespace>syncfusion.ganttdemos.wpf</RootNamespace>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Common\syncfusion.demoscommon.wpf_lib.csproj" />
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
    <PackageReference Include="Syncfusion.Gantt.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfGrid.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Grid.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.GridCommon.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfChart.WPF" Version="31.2.2" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Images/**" />
    <Content Include="Assets\gantt\ProjectData.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <ApplicationDefinition Remove="App.xaml" />
    <Compile Remove="App.xaml.cs" />
    <None Remove="App.config" />
    <Compile Remove="Behaviors/ProjectStatisticsInitialSetupBehavior.cs" />
  </ItemGroup>
  <Import Project="..\Common\demos.common.props" />
  <Import Project="..\targets\MultiTargeting.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>