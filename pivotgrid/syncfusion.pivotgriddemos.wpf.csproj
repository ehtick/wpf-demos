<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.pivotgriddemos.wpfapplication</AssemblyName>
    <RootNamespace>syncfusion.pivotgriddemos.wpfapplication</RootNamespace>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml">
    </Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Data.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Grid.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.GridCommon.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Linq.Base" Version="31.2.2" />
    <PackageReference Include="Syncfusion.PivotAnalysis.Base" Version="31.2.2" />
    <PackageReference Include="Syncfusion.PivotTable.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.PivotTableExport.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfGrid.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Shared.WPF.Classic" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Tools.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.DocIO.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Tools.WPF.Classic" Version="31.2.2" />
    <PackageReference Include="Syncfusion.XlsIO.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.OfficeChart.Base" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Compression.Base" Version="31.2.2" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Common\syncfusion.demoscommon.wpf_lib.csproj">
      <Name>syncfusion.demoscommon.wpf_lib</Name>
    </ProjectReference>
    <ProjectReference Include="syncfusion.pivotgriddemos.wpf_lib.csproj">
      <Name>syncfusion.pivotgriddemos.wpf_lib</Name>
    </ProjectReference>
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Behavior\*" />
    <None Remove="Data\pivotgrid\data.csv" />
    <Compile Remove="Helpers\MonthComparer.cs" />
    <Compile Remove="Model\*" />
    <Compile Remove="ViewModel\*" />
    <Page Remove="Views\**" />
    <Compile Remove="Views\**" />
    <Compile Remove="PivotGridDemosViewModel.cs" />
    <None Remove="Properties\Settings.settings" />
    <None Remove="packages.config" />
    <None Remove="App.config" />
  </ItemGroup>
  <Import Project="..\Common\demos.common.props">
  </Import>
  <Import Project="..\targets\MultiTargeting.targets" />
</Project>