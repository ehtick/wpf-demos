<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.unitconverter.wpf.app</AssemblyName>
    <RootNamespace>syncfusion.unitconverter.wpf.app</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Assets\**" />
    <Compile Remove="Converter\**" />
    <Compile Remove="ViewModel\**" />
    <Compile Remove="Views\**" />
    <EmbeddedResource Remove="Assets\**" />
    <EmbeddedResource Remove="Converter\**" />
    <EmbeddedResource Remove="ViewModel\**" />
    <EmbeddedResource Remove="Views\**" />
    <None Remove="Assets\**" />
    <None Remove="Converter\**" />
    <None Remove="ViewModel\**" />
    <None Remove="Views\**" />
    <Page Remove="Assets\**" />
    <Page Remove="Converter\**" />
    <Page Remove="ViewModel\**" />
    <Page Remove="Views\**" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="syncfusion.unitconverter.wpf_lib.csproj">
      <Name>syncfusion.unitconverter.wpf_lib</Name>
    </ProjectReference>
    <Reference Include="Syncfusion.SfChromelessWindow.Wpf">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfChromelessWindow.Wpf.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Shared.Wpf">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Shared.Wpf.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Tools.Wpf">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Tools.Wpf.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml" />
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
	<RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOption" Value="true" />
  </ItemGroup>
  <PropertyGroup>
    <ApplicationIcon>App.ico</ApplicationIcon>
  </PropertyGroup>
  <Import Project="..\..\targets\MultiTargeting.targets" />
</Project>