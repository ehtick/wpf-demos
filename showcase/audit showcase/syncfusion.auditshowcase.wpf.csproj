<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<AssemblyName>AuditShowcaseDemo</AssemblyName>
		<RootNamespace>AuditShowcaseDemo</RootNamespace>
	</PropertyGroup>

	<ItemGroup Condition=" '$(TargetFramework)' == '$(TargetFramework)-windows' ">
		<RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="syncfusion.auditshowcase.wpf_lib.csproj">
			<Name>syncfusion.auditshowcase.wpf_lib</Name>
		</ProjectReference>
		<None Remove="App.config" />
		<None Remove="Assets\AuditShowcase\*" />
		<Compile Remove="Command\BaseCommand.cs" />
		<Compile Remove="ViewModel\ViewModel.cs" />
		<Page Remove="AuditShowcaseDemo.xaml" />
		<Compile Remove="AuditShowcaseDemo.xaml.cs" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="Syncfusion.PMML.Base">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.PMML.Base.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.Shared.Wpf">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Shared.Wpf.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.Tools.Wpf">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Tools.Wpf.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.SfSkinManager.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfSkinManager.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.Themes.MaterialLight.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Themes.MaterialLight.WPF.dll</HintPath>
		</Reference>
	</ItemGroup>

	<Import Project="..\..\targets\MultiTargeting.targets" />
</Project>