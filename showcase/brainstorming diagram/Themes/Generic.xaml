    <Style TargetType="view:QuickCommand">
        <Setter Property="IsTabStop" Value="false"/>
        <Setter Property="Foreground" Value="{StaticResource SelectorQuickCommandForegroundThumbBrush}"/>
        <Setter Property="FontFamily" Value="Segoe UI Symbol"/>
        <Setter Property="FontWeight" Value="Normal"/>
        <Setter Property="FontSize" Value="1"/>
        <Setter Property="ShapeStyle"
                Value="{Binding ShapeStyle}"></Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="view:QuickCommand">
                    <Border x:Name="QuickCommand" 
        BorderBrush="{TemplateBinding BorderBrush}"
        BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid  x:Name="RootGrid" Background="Transparent">
                            <Path Width="19" Height="19" Data="{Binding DataContext.Shape,RelativeSource={RelativeSource TemplatedParent}}" 
                                 Style="{TemplateBinding ShapeStyle}" 
                                   Stretch="Fill" x:Name="BackgroundGlyph"/>
