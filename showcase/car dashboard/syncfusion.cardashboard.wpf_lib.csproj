<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>syncfusion.cardashboard.wpf</AssemblyName>
    <RootNamespace>syncfusion.cardashboard.wpf</RootNamespace>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.SfGauge.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Themes.MaterialDark.WPF" Version="31.2.2" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Web.dll" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Assets/Car Dashboard/*" />
  </ItemGroup>
  <ItemGroup>
    <ApplicationDefinition Remove="App.xaml" />
    <Compile Remove="App.xaml.cs" />
    <None Remove="App.config" />
  </ItemGroup>
  <Import Project="..\..\targets\MultiTargeting.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>