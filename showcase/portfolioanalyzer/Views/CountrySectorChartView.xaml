<ContentControl x:Class="syncfusion.portfolioanalyzerdemo.wpf.CountrySectorChartView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" Margin="2"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:syncfusion="http://schemas.syncfusion.com/wpf"  Content="{Binding ChartType}"
    syncfusion:DockingManager.Header="Country Sector Chart Module"
    Name="PortfolioGrid"  FontFamily="Verdana"
    syncfusion:DockingManager.SideInDockedMode="Bottom"
    syncfusion:DockingManager.State="Dock"
    syncfusion:DockingManager.TargetNameInDockedMode="PortfolioGrid"
    syncfusion:DockingManager.DesiredWidthInDockedMode="600">
</ContentControl>
