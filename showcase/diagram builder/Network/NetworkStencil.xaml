                <ResourceDictionary Source="/Syncfusion.SfDiagram.Wpf;component/Resources/BasicShapes.xaml" />
                <ResourceDictionary Source="/Syncfusion.Themes.FluentLight.WPF;component/Stencil/Stencil.xaml" />
                <ResourceDictionary Source="/syncfusion.diagrambuilder.wpf;component/Network/NetworkDiagramResources.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
            <!--Style for Symbol-->
            <Style TargetType="syncfusion:Symbol" BasedOn="{StaticResource SyncfusionSymbolStyle}" >
                <Setter Property="Width" Value="70"/>
                <Setter Property="Height" Value="90"/>
                <Setter Property="Margin" Value="4,0,8,0"/>
            </Style>
        </ResourceDictionary>
    </UserControl.Resources>
