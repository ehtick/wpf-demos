<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>syncfusion.diagrambuilder.wpf</AssemblyName>
    <RootNamespace>syncfusion.diagrambuilder.wpf</RootNamespace>
    <Deterministic>true</Deterministic>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Runtime.Serialization" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml">
      <RequiredTargetFramework>4.0</RequiredTargetFramework>
    </Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Resources\App.ico" />
    <None Remove="Asset\DiagramBuilder.png" />
    <Resource Include="Asset\DiagramBuilder.png" />
    <None Remove="Resources\BlankDiagram.png" />
    <Resource Include="Resources\BlankDiagram.png" />
    <None Remove="Resources\Brainstorming.png" />
    <Resource Include="Resources\Brainstorming.png" />
    <None Remove="Resources\Flow_diagram.png" />
    <Resource Include="Resources\Flow_diagram.png" />
    <None Remove="Resources\OrganizationChart.png" />
    <Resource Include="Resources\OrganizationChart.png" />
    <None Remove="Resources\bpmneditor.png" />
    <Resource Include="Resources\bpmneditor.png" />
    <None Remove="Resources\logicCircuitDiagram.png" />
    <Resource Include="Resources\logicCircuitDiagram.png" />
    <None Remove="Resources\NetworkDiagram.png" />
    <Resource Include="Resources\NetworkDiagram.png" />
    <None Remove="App.config" />
    <Compile Remove="App.xaml.cs" />
    <ApplicationDefinition Remove="App.xaml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Licensing" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfDiagram.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfDiagramRibbon.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfInput.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfRadialMenu.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Themes.FluentLight.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Tools.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Shared.WPF.Classic" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Tools.WPF.Classic" Version="31.2.2" />
  </ItemGroup>
  <Import Project="..\..\targets\MultiTargeting.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>