<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.diagrambuilder.wpf.app</AssemblyName>
    <RootNamespace>syncfusion.diagrambuilder.wpf.app</RootNamespace>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <Deterministic>true</Deterministic>
  </PropertyGroup>

<ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
		<RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
</ItemGroup>
 
<ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Runtime.Serialization" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml">
      <RequiredTargetFramework>4.0</RequiredTargetFramework>
    </Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
	</ItemGroup>
  
<ItemGroup>
  <Compile Remove="Asset\**" />
  <Compile Remove="BPMN\**" />
  <Compile Remove="FlowChart\**" />
  <Compile Remove="LogicCircuit\**" />
  <Compile Remove="Network\**" />
  <Compile Remove="obj_47\**" />
  <Compile Remove="Resources\**" />
  <Compile Remove="Utility\**" />
  <Compile Remove="ViewModel\**" />
  <Compile Remove="View\**" />
  <EmbeddedResource Remove="Asset\**" />
  <EmbeddedResource Remove="BPMN\**" />
  <EmbeddedResource Remove="FlowChart\**" />
  <EmbeddedResource Remove="LogicCircuit\**" />
  <EmbeddedResource Remove="Network\**" />
  <EmbeddedResource Remove="obj_47\**" />
  <EmbeddedResource Remove="Resources\**" />
  <EmbeddedResource Remove="Utility\**" />
  <EmbeddedResource Remove="ViewModel\**" />
  <EmbeddedResource Remove="View\**" />
  <None Remove="Asset\**" />
  <None Remove="BPMN\**" />
  <None Remove="FlowChart\**" />
  <None Remove="LogicCircuit\**" />
  <None Remove="Network\**" />
  <None Remove="obj_47\**" />
  <None Remove="Resources\**" />
  <None Remove="Utility\**" />
  <None Remove="ViewModel\**" />
  <None Remove="View\**" />
  <Page Remove="Asset\**" />
  <Page Remove="BPMN\**" />
  <Page Remove="FlowChart\**" />
  <Page Remove="LogicCircuit\**" />
  <Page Remove="Network\**" />
  <Page Remove="Resources\**" />
  <Page Remove="View\**" />
</ItemGroup>

   <ItemGroup>
    <ProjectReference Include="syncfusion.diagrambuilder.wpf_lib.csproj">
      <Name>syncfusion.diagrambuilder.wpf_lib</Name>
    </ProjectReference>        
  </ItemGroup>
  
<ItemGroup>
    <Reference Include="Syncfusion.Licensing">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Licensing.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.SfDiagram.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfDiagram.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.SfDiagramRibbon.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfDiagramRibbon.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.SfInput.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfInput.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.SfRadialMenu.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfRadialMenu.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.SfSkinManager.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfSkinManager.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Shared.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Shared.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Shared.WPF.Classic">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Shared.WPF.Classic.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Themes.FluentLight.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Themes.FluentLight.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Tools.WPF.Classic">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Tools.WPF.Classic.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Tools.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Tools.WPF.dll</HintPath>
    </Reference>
</ItemGroup>
  
  <Import Project="..\..\targets\MultiTargeting.targets" />
</Project>