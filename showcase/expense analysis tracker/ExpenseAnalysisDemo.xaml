<syncfusion:ChromelessWindow x:Class="syncfusion.expenseanalysis.wpf.ExpenseAnalysisDemo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:syncfusion.expenseanalysis.wpf"
        xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
        xmlns:syncfusionskin ="clr-namespace:Syncfusion.SfSkinManager;assembly=Syncfusion.SfSkinManager.WPF"
        mc:Ignorable="d"
        Icon="/syncfusion.expenseanalysis.wpf;component/Assets/ExpenseAnalysis/Icon.png"
        TitleBarBackground="#1E40AF"
        TitleBarForeground="#FFFFFF"
        WindowState="Maximized"
        Title="Expense Analysis"
        Foreground="#374151"
        FontFamily="Segoe UI"
        MinWidth="1250"
        MinHeight="650"
        CloseButtonTemplate="{DynamicResource ProductsWindowCloseButtonTemplate}"
        MaximizeButtonTemplate="{DynamicResource ProductsWindowMaximizeButtonTemplate}"
        MinimizeButtonTemplate="{DynamicResource ProductsWindowMinimizeButtonTemplate}"
        RestoreButtonTemplate="{DynamicResource ProductsWindowRestoreButtonTemplate}">
    <syncfusion:ChromelessWindow.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/syncfusion.expenseanalysis.wpf;component/Assets/ExpenseAnalysis/ChromelessWindowResource.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </syncfusion:ChromelessWindow.Resources>
    <Grid
        syncfusionskin:SfSkinManager.Theme="{syncfusionskin:SkinManagerExtension ThemeName=Windows11Light}">
        <local:MainView/>
    </Grid>
</syncfusion:ChromelessWindow>
