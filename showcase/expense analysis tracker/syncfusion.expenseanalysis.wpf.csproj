<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<AssemblyName>syncfusion.expenseanalysis.wpf.app</AssemblyName>
		<RootNamespace>syncfusion.expenseanalysis.wpf</RootNamespace>
	</PropertyGroup>

	<ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
		<RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
	</ItemGroup>

	<ItemGroup>
	    <Reference Include="Syncfusion.SfGauge.Wpf">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfGauge.Wpf.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.SfSkinManager.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfSkinManager.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.Themes.FluentLight.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Themes.FluentLight.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.Themes.Windows11Light.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Themes.Windows11Light.WPF.dll</HintPath>
		</Reference>
		<ProjectReference Include="syncfusion.expenseanalysis.wpf_lib.csproj">
			<Name>syncfusion.expenseanalysis.wpf_lib</Name>
		</ProjectReference>
		<Reference Include="Syncfusion.Licensing">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Licensing.dll</HintPath>
		</Reference>
	</ItemGroup>

	<ItemGroup>
		<Page Remove="Assets\ExpenseAnalysis\*" />
		<None Remove="Assets\ExpenseAnalysis\*" />
		<Compile Remove="Model\*" />
		<Compile Remove="Utility\*" />
		<Page Remove="View\*" />
		<Compile Remove="View\*" />
		<Compile Remove="ViewModel\*" />
		<Page Remove="ExpenseAnalysisDemo.xaml" />
		<Compile Remove="ExpenseAnalysisDemo.xaml.cs" />
	</ItemGroup>

	<Import Project="..\..\targets\MultiTargeting.targets" />
</Project>
