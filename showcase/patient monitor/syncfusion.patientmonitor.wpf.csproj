<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.patientmonitor.wpf.app</AssemblyName>
    <RootNamespace>syncfusion.patientmonitor.wpf</RootNamespace>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Xaml">
    </Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Data.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfChart.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfGrid.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfShared.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Shared.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Themes.MaterialLight.WPF" Version="31.2.2" />
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="syncfusion.patientmonitor.wpf_lib.csproj">
      <Name>syncfusion.patientmonitor.wpf_lib</Name>
    </ProjectReference>
  </ItemGroup>
  <ItemGroup>
    <None Remove="app.config" />
    <None Remove="Properties\Settings.settings" />
    <None Remove="Assets\PatientMonitor\PatientMonitor.png" />
    <Compile Remove="Behaviors\**" />
    <Compile Remove="Converters\**" />
    <Compile Remove="Model\**" />
    <Page Remove="View\**" />
    <Compile Remove="View\**" />
    <Compile Remove="ViewModel\**" />
    <Page Remove="PatientMonitor.xaml" />
    <Compile Remove="PatientMonitor.xaml.cs" />
  </ItemGroup>
  <Import Project="..\..\targets\MultiTargeting.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>