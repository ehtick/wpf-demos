<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.formulaanalysis.wpf.app</AssemblyName>
    <RootNamespace>syncfusion.formulaanalysis.wpf</RootNamespace>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="PresentationFramework.Aero" />
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Xaml">
    </Reference>
    <Reference Include="UIAutomationProvider" />
    <Reference Include="UIAutomationTypes" />
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Calculate.Base" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Grid.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.GridCommon.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Linq.Base" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Shared.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfShared.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfInput.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Tools.Wpf" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Themes.MaterialLight.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Themes.MaterialDarkBlue.WPF" Version="31.2.2" />
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="syncfusion.formulaanalysis.wpf_lib.csproj">
      <Name>syncfusion.formulaanalysis.wpf_lib</Name>
    </ProjectReference>
  </ItemGroup>
  <ItemGroup>
    <None Remove="app.config" />
    <None Remove="Properties\Settings.settings" />
    <None Remove="Assets\FormulaAnalysis\*" />
    <Compile Remove="Behavior\FormulaAnalysisBehavior.cs" />
    <Compile Remove="Helpers\CalcGrid.cs" />
    <Compile Remove="Model\Model.cs" />
    <Compile Remove="ViewModel\ViewModel.cs" />
    <Page Remove="FormulaAnalysis.xaml" />
    <Compile Remove="FormulaAnalysis.xaml.cs" />
  </ItemGroup>
  <Import Project="..\..\targets\MultiTargeting.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>