<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.workfloweditor.wpf.app</AssemblyName>
    <RootNamespace>syncfusion.workfloweditor.wpf.app</RootNamespace>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <Deterministic>true</Deterministic>
  </PropertyGroup>

<ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
	<RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
</ItemGroup>
  
<ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml">
      <RequiredTargetFramework>4.0</RequiredTargetFramework>
    </Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
</ItemGroup>
  
<ItemGroup>
  <Compile Remove="Asset\**" />
  <Compile Remove="Data\**" />
  <Compile Remove="obj_47\**" />
  <Compile Remove="Resource\**" />
  <Compile Remove="Template\**" />
  <Compile Remove="Utility\**" />
  <Compile Remove="ViewModel\**" />
  <Compile Remove="Views\**" />
  <EmbeddedResource Remove="Asset\**" />
  <EmbeddedResource Remove="Data\**" />
  <EmbeddedResource Remove="obj_47\**" />
  <EmbeddedResource Remove="Resource\**" />
  <EmbeddedResource Remove="Template\**" />
  <EmbeddedResource Remove="Utility\**" />
  <EmbeddedResource Remove="ViewModel\**" />
  <EmbeddedResource Remove="Views\**" />
  <None Remove="Asset\**" />
  <None Remove="Data\**" />
  <None Remove="obj_47\**" />
  <None Remove="Resource\**" />
  <None Remove="Template\**" />
  <None Remove="Utility\**" />
  <None Remove="ViewModel\**" />
  <None Remove="Views\**" />
  <Page Remove="Asset\**" />
  <Page Remove="Data\**" />
  <Page Remove="obj_47\**" />
  <Page Remove="Resource\**" />
  <Page Remove="Template\**" />
  <Page Remove="Utility\**" />
  <Page Remove="ViewModel\**" />
  <Page Remove="Views\**" />
</ItemGroup>

  <ItemGroup>     
    <ProjectReference Include="syncfusion.workfloweditor.wpf_lib.csproj">
      <Name>syncfusion.workfloweditor.wpf_lib</Name>
    </ProjectReference>
  </ItemGroup>

<ItemGroup>
    <Reference Include="Syncfusion.Licensing">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Licensing.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.SfBusyIndicator.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfBusyIndicator.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.SfColorPalette.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfColorPalette.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.SfDiagram.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfDiagram.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.SfInput.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfInput.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Shared.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Shared.WPF.dll</HintPath>
    </Reference>
</ItemGroup>

  <Import Project="..\..\targets\MultiTargeting.targets" />
  
</Project>