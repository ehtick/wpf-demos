<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>library</OutputType>
    <AssemblyName>syncfusion.workfloweditor.wpf</AssemblyName>
    <RootNamespace>syncfusion.workfloweditor.wpf</RootNamespace>
    <Deterministic>true</Deterministic>
    <TargetFrameworkVersion>
    </TargetFrameworkVersion>
  </PropertyGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Runtime.Serialization" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml">
      <RequiredTargetFramework>4.0</RequiredTargetFramework>
    </Reference>
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Asset\**" />
    <Compile Remove="obj_47\**" />
    <EmbeddedResource Remove="Asset\**" />
    <EmbeddedResource Remove="obj_47\**" />
    <None Remove="Asset\**" />
    <None Remove="obj_47\**" />
    <Page Remove="Asset\**" />
    <Page Remove="obj_47\**" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Resource\work-flow.png" />
    <Resource Include="Resource\bg_grid_texture.png" />
    <Resource Include="Resource\Work-flow1.png" />
    <Resource Include="Resource\App.ico" />
    <EmbeddedResource Include="Data\Diagram\Doctor appointment.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Data\Diagram\Make ice cream.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Data\Diagram\Support Workflow.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <ApplicationDefinition Remove="App.xaml" />
    <Compile Remove="App.xaml.cs" />
    <None Remove="App.config" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Licensing" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfBusyIndicator.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfColorPalette.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfDiagram.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.SfInput.WPF" Version="31.2.2" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="31.2.2" />
  </ItemGroup>
  <Import Project="..\..\targets\MultiTargeting.targets" />
  <ItemGroup>
    <None Include="packages.config" />
  </ItemGroup>
</Project>