<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<AssemblyName>syncfusion.visualstudiodemo.wpfapplication</AssemblyName>
		<RootNamespace>syncfusion.visualstudiodemo.wpfapplication</RootNamespace>
	</PropertyGroup>

	<ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
		<RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
	</ItemGroup>

	<ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
		<Reference Include="System" />
		<Reference Include="System.Data" />
		<Reference Include="System.Xml" />
		<Reference Include="Microsoft.CSharp" />
		<Reference Include="System.Core" />
		<Reference Include="System.Xml.Linq" />
		<Reference Include="System.Data.DataSetExtensions" />
		<Reference Include="System.Net.Http" />
		<Reference Include="System.Xaml" />
		<Reference Include="Syncfusion.SfSkinManager.WPF">
		  <Name>Syncfusion.SfSkinManager.WPF</Name>
		  <Private>True</Private>
		</Reference>
		<Reference Include="Syncfusion.Themes.Office2019Colorful.WPF">
		  <Name>Syncfusion.Themes.Office2019Colorful.WPF</Name>
		  <Private>True</Private>
		</Reference>
		<Reference Include="WindowsBase" />
		<Reference Include="PresentationCore" />
		<Reference Include="PresentationFramework" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Remove="Assets\**" />
	  <Compile Remove="Library\**" />
	  <Compile Remove="Model\**" />
	  <Compile Remove="ViewModel\**" />
	  <Compile Remove="Views\**" />
	  <EmbeddedResource Remove="Assets\**" />
	  <EmbeddedResource Remove="Library\**" />
	  <EmbeddedResource Remove="Model\**" />
	  <EmbeddedResource Remove="ViewModel\**" />
	  <EmbeddedResource Remove="Views\**" />
	  <None Remove="Assets\**" />
	  <None Remove="Library\**" />
	  <None Remove="Model\**" />
	  <None Remove="ViewModel\**" />
	  <None Remove="Views\**" />
	  <Page Remove="Assets\**" />
	  <Page Remove="Library\**" />
	  <Page Remove="Model\**" />
	  <Page Remove="ViewModel\**" />
	  <Page Remove="Views\**" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="Syncfusion.SfSkinManager.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfSkinManager.WPF.dll</HintPath>
		</Reference>
		<Reference Include="Syncfusion.Themes.Office2019Colorful.WPF">
			<HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Themes.Office2019Colorful.WPF.dll</HintPath>
		</Reference>
		<ProjectReference Include="syncfusion.visualstudiodemo.wpf_lib.csproj">
			<Name>syncfusion.visualstudiodemo.wpf_lib</Name>
		</ProjectReference>
	</ItemGroup>

	<PropertyGroup>
		<ApplicationIcon>App.ico</ApplicationIcon>
	</PropertyGroup>

	<Import Project="..\..\targets\MultiTargeting.targets" />
</Project>