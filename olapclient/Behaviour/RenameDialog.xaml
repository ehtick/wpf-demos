<Window
    x:Class="syncfusion.olapclientdemos.wpf.RenameDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Rename Dialog"
    Width="300"
    Height="130"
    WindowStartupLocation="CenterOwner">
    <Grid Margin="7">
        <Grid.RowDefinitions>
            <RowDefinition Height="23*" />
            <RowDefinition Height="25*" />
            <RowDefinition Height="35*" />
        </Grid.RowDefinitions>
        <TextBlock
            Name="textBlock1"
            Grid.RowSpan="1"
            Height="23"
            HorizontalAlignment="Left"
            VerticalAlignment="Bottom"
            Text="Name :" />
        <TextBox
            Name="textBox1"
            Grid.Row="1"
            Grid.RowSpan="1"
            Height="23"
            VerticalAlignment="Center"
            KeyDown="textBox1_KeyDown" />
        <Button
            Name="button1"
            Grid.Row="2"
            Width="75"
            Height="23"
            Margin="0,7,0,0"
            HorizontalAlignment="Right"
            VerticalAlignment="Center"
            Click="button1_Click"
            Content="OK" />
    </Grid>
</Window>
