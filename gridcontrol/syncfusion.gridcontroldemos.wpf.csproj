		<Page Remove="Themes\Generic.xaml" />
		<Compile Remove="Zooming\**" />
		<Page Remove="Zooming\**" />
		<Compile Remove="GridControlDemosViewModel.cs" />
	</ItemGroup>
	<Import Project="..\common\demos.common.props">
	</Import>
	<Import Project="..\targets\MultiTargeting.targets" />
</Project>