<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.dropdowndemos.wpfapplication</AssemblyName>
    <RootNamespace>syncfusion.dropdowndemos.wpf</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="AutoComplete\**" />
    <Compile Remove="CheckListBox\**" />
    <Compile Remove="ComboBoxAdv\**" />
    <Compile Remove="Multi Column Dropdown\**" />
    <EmbeddedResource Remove="AutoComplete\**" />
    <EmbeddedResource Remove="CheckListBox\**" />
    <EmbeddedResource Remove="ComboBoxAdv\**" />
    <EmbeddedResource Remove="Multi Column Dropdown\**" />
    <None Remove="AutoComplete\**" />
    <None Remove="CheckListBox\**" />
    <None Remove="ComboBoxAdv\**" />
    <None Remove="Multi Column Dropdown\**" />
    <Page Remove="AutoComplete\**" />
    <Page Remove="CheckListBox\**" />
    <Page Remove="ComboBoxAdv\**" />
    <Page Remove="Multi Column Dropdown\**" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="DropDownDemosViewModel.cs" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\common\syncfusion.demoscommon.wpf_lib.csproj">
      <Name>syncfusion.demoscommon.wpf_lib</Name>
    </ProjectReference>
    <ProjectReference Include="syncfusion.dropdowndemos.wpf_lib.csproj">
      <Name>syncfusion.dropdowndemos.wpf_lib</Name>
    </ProjectReference>
    <Reference Include="Syncfusion.Data.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Data.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.SfGrid.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfGrid.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Tools.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.Tools.WPF.dll</HintPath>
    </Reference>
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.19" />
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK'">
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Xml" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xaml" />
    <Reference Include="WindowsBase" />
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
	<RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOption" Value="true" />
  </ItemGroup>
  <PropertyGroup>
    <ApplicationIcon>App.ico</ApplicationIcon>
  </PropertyGroup>
  <Import Project="..\common\demos.common.props" />
  <Import Project="..\targets\MultiTargeting.targets" />
</Project>