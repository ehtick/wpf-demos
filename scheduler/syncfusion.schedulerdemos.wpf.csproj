<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <AssemblyName>syncfusion.schedulerdemos.wpfapplication</AssemblyName>
    <RootNamespace>syncfusion.schedulerdemos.wpf</RootNamespace>
  </PropertyGroup>

   <ItemGroup Condition=" '$(TargetFramework)' == 'net9.0-windows' ">
    <RuntimeHostConfigurationOption Include="Switch.System.Windows.Controls.DisableDynamicResourceOptimization" Value="true" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\common\syncfusion.demoscommon.wpf_lib.csproj" />
    <ProjectReference Include="syncfusion.schedulerdemos.wpf_lib.csproj" />
    <Reference Include="Syncfusion.SfScheduler.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\$(TargetFramework)\Syncfusion.SfScheduler.WPF.dll</HintPath>
    </Reference>
  </ItemGroup>
  
  <ItemGroup  Condition=" '$(TargetFrameworkIdentifier)' == '.NETFRAMEWORK' ">
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Net.Http" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Behaviors\*" />
	  <Page Remove="Helper\*" />
	  <Compile Remove="Views\*" />
    <Compile Remove="ViewModel\*" />
    <Compile Remove="Helper\*" />
    <Compile Remove="SchedulerDemosViewModel.cs"/>
    <None Remove="packages.config" />
    <Page Remove="Views\**" />
	  <Compile Remove="Model\*" />
	  <Compile Remove="Converter\*" />
  </ItemGroup>

  <Import Project="..\targets\MultiTargeting.targets" />
  <Import Project="..\common\demos.common.props" />
 
</Project>